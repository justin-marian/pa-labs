all: hanoi find_max merge_sort occurences binary_search

hanoi: ../hanoi.c
	gcc ../hanoi.c -o hanoi

find_max: ../find_max.c
	gcc ../find_max.c -o find_max

merge_sort: ../merge_sort.c
	gcc ../merge_sort.c -o merge_sort

occurences: ../occurences.c
	gcc ../occurences.c -o occurences

binary_search: ../binary_search.c
	gcc ../binary_search.c -o binary_search

clean:
	rm -rf hanoi find_max merge_sort occurences binary_search
